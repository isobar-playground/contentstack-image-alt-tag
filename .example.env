# Contentstack API Configuration
# Your Contentstack API key (found in Settings > Stack)
CONTENTSTACK_API_KEY=your_api_key_here

# Contentstack Management Token
# Management token with write permissions (found in Settings > Tokens > Management Tokens)
# This token is required for updating asset descriptions
CONTENTSTACK_MANAGEMENT_TOKEN=your_management_token_here

# Contentstack Host (optional)
# Host URL for your Contentstack region (e.g., "api.contentstack.io" for NA, "eu-api.contentstack.com" for EU)
# If not specified, defaults to NA region
CONTENTSTACK_HOST=api.contentstack.io

# Contentstack Environment (optional)
# The environment name where assets are stored (e.g., "production", "development")
CONTENTSTACK_ENVIRONMENT=your_environment_here

# OpenAI API Configuration
# Your OpenAI API key (found at https://platform.openai.com/api-keys)
# Required for generating ALT tags using GPT-4o vision model
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Model (optional)
# The model to use for generating ALT tags (e.g., "gpt-4o", "gpt-4o-mini", "gpt-4-vision-preview")
# If not specified, defaults to "gpt-4o"
OPENAI_MODEL=gpt-4o

