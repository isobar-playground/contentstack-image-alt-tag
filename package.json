{
  "name": "contentstack-image-alt-tag",
  "version": "1.0.0",
  "description": "Tool to generate and update ALT tags for images in Contentstack",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "step1": "node scripts/step1-get-languages.js",
    "step2": "node scripts/step2-get-images.js",
    "step3": "node scripts/step3-filter-fields.js",
    "step4": "node scripts/step4-generate-alt-tags.js",
    "step5": "node scripts/step5-wait-for-batch.js",
    "step6": "node scripts/step6-confirm-updates.js",
    "step7": "node scripts/step7-update-contentstack.js",
    "all": "npm run reset && npm run step1 && npm run step2 && npm run step3 && npm run step4 && npm run step5 && npm run step6 && npm run step7",
    "reset": "node scripts/reset.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "private",
  "dependencies": {
    "@contentstack/management": "^1.14.0",
    "dotenv": "^16.4.5",
    "openai": "^4.28.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  }
}
